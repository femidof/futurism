{
    "manifest_version": 2,
    "name": "User Feedback",
    "description": "This extension aids in user testing.",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "content_scripts": [{
        "matches": ["https://www.google.com/*"],
        "css": ["main.css"]
    }],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "My custom google page!"
    },
    "permissions": ["activeTab"]
}